    // 0409 = English (United States), 04E4 = Windows Multilingual
    BLOCK "040904E4"
    BEGIN
      VALUE "CompanyName", "FrankCasanova Technologies"
      VALUE "FileDescription", "Rustganizer â€“ console file organizer"
      VALUE "FileVersion", "0.2.0.0"
      VALUE "InternalName", "rustganizer"
