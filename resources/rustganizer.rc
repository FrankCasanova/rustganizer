      VALUE "ProductName", "Rustganizer"
      VALUE "ProductVersion", "0.2.0.0"
      VALUE "LegalCopyright", "Copyright 2025 FrankCasanova Technologies"
      VALUE "Comments", "Built with Rust"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1252
  END
END
